[
  {"name": "nginx", "category": "web-server", "headers": [{"name": "Server", "pattern": "nginx"}]},
  {"name": "Apache", "category": "web-server", "headers": [{"name": "Server", "pattern": "Apache"}]},
  {"name": "IIS", "category": "web-server", "headers": [{"name": "Server", "pattern": "Microsoft-IIS"}]},
  {"name": "LiteSpeed", "category": "web-server", "headers": [{"name": "Server", "pattern": "LiteSpeed"}]},
  {"name": "Caddy", "category": "web-server", "headers": [{"name": "Server", "pattern": "Caddy"}]},
  {"name": "Traefik", "category": "web-server", "headers": [{"name": "Server", "pattern": "Traefik"}]},
  {"name": "Kestrel", "category": "web-server", "headers": [{"name": "Server", "pattern": "Kestrel"}]},
  {"name": "Envoy", "category": "web-server", "headers": [{"name": "Server", "pattern": "envoy"}]},
  {"name": "openresty", "category": "web-server", "headers": [{"name": "Server", "pattern": "openresty"}]},
  {"name": "Tengine", "category": "web-server", "headers": [{"name": "Server", "pattern": "Tengine"}]},

  {"name": "PHP", "category": "language", "headers": [{"name": "X-Powered-By", "pattern": "PHP"}]},
  {"name": "ASP.NET", "category": "language", "headers": [{"name": "X-Powered-By", "pattern": "ASP\\.NET"}]},
  {"name": "ASP.NET", "category": "language", "headers": [{"name": "X-AspNet-Version", "pattern": ".+"}]},
  {"name": "Java", "category": "language", "headers": [{"name": "X-Powered-By", "pattern": "JSP|Servlet|Java"}]},
  {"name": "Python", "category": "language", "headers": [{"name": "Server", "pattern": "Python|Werkzeug|gunicorn|uvicorn"}]},
  {"name": "Node.js", "category": "language", "headers": [{"name": "X-Powered-By", "pattern": "Express"}]},
  {"name": "Ruby", "category": "language", "headers": [{"name": "X-Powered-By", "pattern": "Phusion Passenger"}]},
  {"name": "Go", "category": "language", "headers": [{"name": "Server", "pattern": "^Go-http"}]},

  {"name": "React", "category": "framework", "body": ["__NEXT_DATA__", "react-root", "_reactRootContainer", "react.production.min"]},
  {"name": "Next.js", "category": "framework", "headers": [{"name": "X-Powered-By", "pattern": "Next\\.js"}], "body": ["__NEXT_DATA__", "_next/static"]},
  {"name": "Vue.js", "category": "framework", "body": ["Vue.js", "v-cloak", "__vue__", "vue.runtime"]},
  {"name": "Nuxt", "category": "framework", "body": ["__NUXT__", "_nuxt/"]},
  {"name": "Angular", "category": "framework", "body": ["ng-version=", "ng-app=", "angular.min.js"]},
  {"name": "Svelte", "category": "framework", "body": ["svelte-", "__svelte"]},
  {"name": "Django", "category": "framework", "headers": [{"name": "X-Frame-Options", "pattern": "DENY"}], "cookies": ["csrftoken"]},
  {"name": "Laravel", "category": "framework", "cookies": ["laravel_session", "XSRF-TOKEN"]},
  {"name": "Rails", "category": "framework", "headers": [{"name": "X-Powered-By", "pattern": "Phusion Passenger"}], "cookies": ["_rails_session", "_session_id"]},
  {"name": "Spring", "category": "framework", "headers": [{"name": "X-Application-Context", "pattern": ".+"}]},
  {"name": "Flask", "category": "framework", "headers": [{"name": "Server", "pattern": "Werkzeug"}]},
  {"name": "FastAPI", "category": "framework", "headers": [{"name": "Server", "pattern": "uvicorn"}]},

  {"name": "WordPress", "category": "cms", "body": ["wp-content/", "wp-includes/", "wp-json"]},
  {"name": "Drupal", "category": "cms", "headers": [{"name": "X-Generator", "pattern": "Drupal"}], "body": ["Drupal.settings", "sites/default/files"]},
  {"name": "Joomla", "category": "cms", "body": ["/media/jui/", "Joomla!", "/administrator/"]},
  {"name": "Shopify", "category": "cms", "body": ["cdn.shopify.com", "Shopify.theme"], "headers": [{"name": "X-ShopId", "pattern": ".+"}]},
  {"name": "Squarespace", "category": "cms", "body": ["squarespace.com", "static.squarespace.com"]},
  {"name": "Ghost", "category": "cms", "body": ["ghost-", "content/themes/"], "headers": [{"name": "X-Powered-By", "pattern": "Ghost"}]},
  {"name": "Wix", "category": "cms", "body": ["wix.com", "X-Wix-"]},
  {"name": "Webflow", "category": "cms", "body": ["webflow.com", "wf-page"]},
  {"name": "HubSpot", "category": "cms", "body": ["hs-scripts.com", "hubspot"]},
  {"name": "Contentful", "category": "cms", "body": ["contentful.com", "ctfl"]},

  {"name": "Cloudflare", "category": "cdn-waf", "headers": [{"name": "Server", "pattern": "cloudflare"}, {"name": "CF-RAY", "pattern": ".+"}]},
  {"name": "Akamai", "category": "cdn-waf", "headers": [{"name": "X-Akamai-Transformed", "pattern": ".+"}, {"name": "Server", "pattern": "AkamaiGHost"}]},
  {"name": "CloudFront", "category": "cdn-waf", "headers": [{"name": "X-Amz-Cf-Id", "pattern": ".+"}, {"name": "X-Amz-Cf-Pop", "pattern": ".+"}]},
  {"name": "Fastly", "category": "cdn-waf", "headers": [{"name": "X-Served-By", "pattern": "cache-"}, {"name": "X-Fastly-Request-ID", "pattern": ".+"}]},
  {"name": "Vercel", "category": "cdn-waf", "headers": [{"name": "X-Vercel-Id", "pattern": ".+"}, {"name": "Server", "pattern": "Vercel"}]},
  {"name": "Netlify", "category": "cdn-waf", "headers": [{"name": "X-NF-Request-ID", "pattern": ".+"}, {"name": "Server", "pattern": "Netlify"}]},
  {"name": "Sucuri", "category": "cdn-waf", "headers": [{"name": "X-Sucuri-ID", "pattern": ".+"}, {"name": "Server", "pattern": "Sucuri"}]},
  {"name": "Incapsula", "category": "cdn-waf", "headers": [{"name": "X-CDN", "pattern": "Incapsula"}]},
  {"name": "AWS ELB", "category": "cdn-waf", "headers": [{"name": "Server", "pattern": "awselb"}]},

  {"name": "Varnish", "category": "web-server", "headers": [{"name": "Via", "pattern": "varnish"}, {"name": "X-Varnish", "pattern": ".+"}]}
]
